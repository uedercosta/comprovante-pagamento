program ComprovantePagamento;

uses
  Vcl.Forms,
  View.MenuPrincipal in 'View\View.MenuPrincipal.pas' {Form1},
  View.Padrao in 'View\Padrao\View.Padrao.pas' {Form2},
  Repositorio.DataModulo in 'Repositorio\Repositorio.DataModulo.pas' {DataModule1: TDataModule};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TForm1, Form1);
  Application.CreateForm(TForm2, Form2);
  Application.CreateForm(TDataModule1, DataModule1);
  Application.Run;
end.
